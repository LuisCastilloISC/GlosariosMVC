<div class="department_area section_gap" style="
    padding-top: 0px;">

    <div class="content" style="
    margin-left: 100px;
">
        <div class="row align-items-center">
            <div class="col-lg-6">

                <label class="description" for="RNombre" style="font-size:20px">Nombre del glosario</label>
                <div>
                    <input id="RNombre" name="RNombre" class="element text large" type="text" size="70" maxlength="989546" value="" />
                </div>
                <br />
                <br />

                <!-- Tab links -->
                <div class="tab" style=" width: 1600px;">
                    <button class="tablinks" onclick="openCity(event, 'Ingresar texto')">Texto a texto</button>
                    <button class="tablinks" onclick="openCity(event, 'Imagen')">Imagen a texto</button>
                    <!--  <button class="tablinks" onclick="openCity(event, 'Tokyo')">Tokyo</button>-->
                </div>

                <!-- Tab content -->
                <div id="Ingresar texto" class="tabcontent" style="display: block;width: 1600px; ">


                    <form id="form_58909" class="appnitro" method="post" action="">
                        <div class="form_description">


                        </div>
                        <label class="description" style="font-size:20px" for="element_2">Escriba el concepto a agregar</label>
                        <div>
                            <input id="Concepto" name="Concepto" class="element text large" type="text" size="70" maxlength="989546" value="" />
                        </div>




                        <label class="description" for="element_3">Entrada</label>
                        <div>
                            <textarea id="texto" name="texto" cols="134" rows="8"></textarea>
                        </div>
                        <label class="description" for="element_3">Salida </label>
                        <div>
                            <textarea id="textoSalida" name="textoSalida" cols="134" rows="8"></textarea>
                        </div>


                        <p>
                            <input type="hidden" name="form_id" value="58909" />
                            <button type="button" class="btn btn-primary" onclick="Glosario('Usuarios/Rewrite')">Agregar Concepto</button>

                            <input type="hidden" name="form_id" value="58909" />
                            <button type="button" class="btn btn-primary" onclick="PDF('Usuarios/PDF')">Generar PDF</button>
                        </p>
                    </form>
                </div>

                <div id="Imagen" class="tabcontent" style="width: 1600px;">

                    <picture>

                        <img src="~/img/1.PNG" alt="Flowers" style="width:1300px">
                    </picture>
                    <input id="uploadImageButton" data-l10n-id="fileToolbar_uploadImageFile_icon" type="button" class="btn btn-success" title="Upload Image File" value="Upload Image File" name="UploadImageFile" onclick="document.getElementById('uploadImageFile').click();" style="cursor: pointer;" />
                    <input id="uploadImageFile" data-l10n-id="fileToolbar_uploadImageFile_icon" type="file" title="Upload Image File" name="UploadImageFile" style="display:none" />
                    <input type="button" id="recognizeTextButton" onclick="Reconocer('Usuarios/ReconocerTexto')" value="Recognize Text" style="width:100%;" />
                    <div>
                        <textarea id="textoImagen" name="textoImagen" cols="134" rows="8"></textarea>
                    </div>
                    <label class="description" for="element_3">Salida </label>
                    <div>
                        <textarea id="textoSalida" name="textoSalida" cols="134" rows="8"></textarea>
                    </div>
                    <input type="hidden" name="form_id" value="58909" />
                    <button type="button" class="btn btn-primary" onclick="Glosario('Usuarios/Rewrite')">Procesar</button>


                </div>

                <!--<div id="Tokyo" class="tabcontent">
                    <h3>Tokyo</h3>
                    <p>Tokyo is the capital of Japan.</p>
                </div>-->





            </div>
        </div>

    </div>
</div>


